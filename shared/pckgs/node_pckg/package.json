{
  "name": "@monorepo/node_pckg",
  "version": "0.1.0",
  "description": "A Node.js package.",
  "keywords": [
    "node",
    "lib"
  ],
  "author": "moon",
  "license": "MIT",
  "repository": "https://github.com/moonrepo/examples",
  "devDependencies": {
    "packemon":"^3.0.3"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "inputs": {
      "index": "src/index.ts"
    }
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.5.0"
  },
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "exports": {
    "./*": {
      "types": "./lib/*.d.ts",
      "node": {
        "types": "./lib/*.d.ts",
        "default": "./lib/*.js"
      },
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "node": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    }
  }
}
