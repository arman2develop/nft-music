{
    "name": "nft-music-monorepo",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "node:version": "node --version",
        "pnpm:version": "pnpm --version",
        "pnpm:yourpackage": "pnpm view yourpackage versions",        
        "pnpm:clean": "find ./ -name node_modules -type d -exec rm -rf {} +",
        "pnpm:update": "pnpm -r update -i -L",
        "pnpm:dedupe": "pnpm dedupe --check",
        "moon:clean": "moon clean --lifetime '7 days'",
        "moon": "moon  trace --color --log",
        "m:graph": "moon project-graph",
        "check": "moon check --all",
        "format": "moon run :format",
        "typecheck": "moon run :typecheck",
        "test": "moon run :test",
        "lint": "moon run :lint",
        "dev": "moon run :dev",
        "build": "moon run :build",
        "app": "moon run :task --query 'projectType=application'",
        "front:dir": "moon run webfrontclient:build webfrontadmin:build",
        
        "webfrontadmin": "moon run webfrontadmin:build",
        "webfrontclient": "moon run webfrontclient:build",
        "rustapi": "moon run rustapi:build",
        "gaudio": "moon run gaudio:build",
        "browser_pckg": "moon run browser_package:build",
        "node_pckg": "moon run node_package:build",

        
        "webfrontadmin:dev": "moon run webfrontadmin:dev",
        "webfrontclient:dev": "moon run webfrontclient:dev",
        "gaudio:dev": "moon run gaudio:dev",        

        "webfrontadmin:m:graph": "moon project-graph webfrontadmin",
        "webfrontclient:m:graph": "moon project-graph webfrontclient",
        "gaudio:m:graph": "moon project-graph gaudio",
        "browser_pckg:m:graph": "moon project-graph browser_pckg",
        "node_pckg:m:graph": "moon project-graph node_pckg",
        
        "typescript": "moon run :build --query \"language=typescript\"",
        "react": "moon run :build --query \"project~react-*\""
    },
    "workspaces": [
        "api/rustapi",
        "apps/*",
        "shared/pckgs/*",
        "shared/front/*",
        "wasm/gaudio"
    ],
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@moonrepo/cli": "^1.6.0",
        "@swc/core": "^1.3.59",
        "@types/node": "^18.16.0",
        "eslint": "^8.39.0",
        "eslint-config-moon": "^2.0.3",
        "packemon": "^3.1.0",
        "prettier": "^2.8.8",
        "prettier-config-moon": "^1.1.2",
        "prettier-plugin-svelte": "^2.10.0",
        "tsconfig-moon": "^1.3.0",
        "typescript": "^5.0.4"
    },
    "packageManager": "pnpm@8.6.12",
    "engines": {
        "node": "18.17.1"
    }
}
