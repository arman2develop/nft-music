{
    "name": "nft-music-monorepo",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "node:version": "node --version",
        "pnpm:version": "pnpm --version",
        "moon": "moon --color --log trace",
        
        "check": "moon check --all",
        "format": "moon run :format",
        "typecheck": "moon run :typecheck",
        "test": "moon run :test",
        "lint": "moon run :lint",
        "dev": "moon run :dev",
        "build": "moon run :build",

        "app": "moon run :task --query 'projectType=application'",
        "front": "moon run webfrontclient:build webfrontadmin:build",
        
        "webfrontadmin": "moon run webfrontadmin:build",
        "webfrontclient": "moon run webfrontclient:build",
        "gaudio": "moon run gaudio:build",
        "browser_package": "moon run browser_package:build",
        "node_package": "moon run node_package:build",

        "webfrontadmin:dev": "moon run webfrontadmin:dev",
        "webfrontclient:dev": "moon run webfrontclient:dev",
        "gaudio:dev": "moon run gaudio:dev",
        
        "typescript": "moon run :task --query \"language=typescript\"",
        "react": "moon run :task --query \"project~react-*\""
    },
    "workspaces": [       
        "apps/*",
        "shared/front/*",        
        "shared/packages/*",        
        "wasm/js/*"
    ],
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@moonrepo/cli": "^1.5.1",
        "@swc/core": "^1.3.56",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.16.3",
        "babel-preset-moon": "^2.0.3",
        "eslint": "^8.39.0",
        "eslint-config-moon": "^2.0.3",
        "jest": "^29.5.0",
        "jest-preset-moon": "^2.0.1",
        "packemon": "^3.0.3",
        "prettier": "^2.8.8",
        "prettier-config-moon": "^1.1.2",
        "prettier-plugin-svelte": "^2.10.0",
        "tsconfig-moon": "^1.3.0",
        "typescript": "^5.0.4"
    },
    "packageManager": "pnpm@8.5.0",
    "engines": {
        "node": "18.16.0"
    }
}
